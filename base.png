<script>
    function throttle(func, ms) {
        let isThrottled = false,
            savedArgs,
            savedThis;

        function wrapper() {
            if (isThrottled) {
                // (2)
                savedArgs = arguments;
                savedThis = this;
                return;
            }

            func.apply(this, arguments); // (1)

            isThrottled = true;

            setTimeout(function () {
                isThrottled = false; // (3)
                if (savedArgs) {
                    wrapper.apply(savedThis, savedArgs);
                    savedArgs = savedThis = null;
                }
            }, ms);
        }

        return wrapper;
    }

    const elem = document.querySelector(".introduction .header");
    window.addEventListener(
        "scroll",
        throttle(function (event) {
            if (window.pageYOffset > 60) {
                elem.classList.remove("header_sticky");
            } else {
                elem.classList.add("header_sticky");
            }
        }, 300),
        {passive: true}
    );

</script>
